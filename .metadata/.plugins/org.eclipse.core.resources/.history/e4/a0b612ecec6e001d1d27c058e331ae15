package Easy;

public class MergeTwoSortedLists {
	 public ListNode mergeTwoLists(ListNode list1, ListNode list2) {
		if(list1 == null)
			return list2;
		
		if(list2 == null)
			return list1;
		
		ListNode rs;
		
		if(list1.val < list2.val) {
			rs = new ListNode(list1.val);
			list1 = list1.next;
		}
		
		else {
			rs = new ListNode(list2.val);
			list2 = list2.next;
		}
			
		while(true) {
			
		}
		 
		 
		 
		 return list2;
	    }
}
