package Easy;

import java.util.HashMap;

public class RomanToInteger {
	public int romanToInt(String s) {
		if(s.length() == 0)
			return 0;
		int n = 0;
		int min;
		int max;
		HashMap<Character, Integer> roman = new HashMap<Character, Integer>();
		roman.put('I', 1);
		roman.put('V', 5);
		roman.put('X', 10);
		roman.put('L', 50);
		roman.put('C', 100);
		roman.put('D', 500);
		roman.put('M', 1000);
		
		
		return n;
	}
}
