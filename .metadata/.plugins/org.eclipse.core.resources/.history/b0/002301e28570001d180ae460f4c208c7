package Easy;

public class ID04 {
	public static double findMedianSortedArrays(int[] nums1, int[] nums2) {
        int sum = 0;
        int m = nums1.length;
        int n =  nums2.length;
        int count = m + n;
        
        for(int i = 0; i < m || i < n; i++) {
        	if(i < m)
        		sum+=nums1[i];
        	
        	if(i <n)
        		sum += nums2[i];
        }
    }
}
