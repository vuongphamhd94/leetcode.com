package Easy;

import java.util.Stack;

public class AddTowNumbers {
	public ListNode addTwoNumbers(ListNode l1, ListNode l2) {
		Stack<Integer> st1 = new Stack<>();
		Stack<Integer> st2 = new Stack<>();
		while(l1!= null) {
			st1.add(l1.val);
			l1 = l1.next;
		}
		while(l2!= null) {
			st1.add(l2.val);
			l2 = l2.next;
		}
	}
}
